    object editHeartbeat: TMaskEdit
      Left = 8
      Top = 32
      Width = 113
      Height = 21
      Hint = 
        'Specify the heartbeat size in milliseconds (1000 = 1 second).'#13#10'T' +
        'his setting takes effect when the current song ends.'#13#10' '#13#10'This is' +
        ' the primary timing mechanism for gen_playtime.'#13#10' '#13#10'Setting this' +
        ' lower than 1000 can result in problems and is'#13#10'not really neces' +
        'sary anyway. 1000, being one second does'#13#10'make the raw statistic' +
        's more understandable.'
      EditMask = '09999;0; '
      MaxLength = 5
      ParentShowHint = False
      ShowHint = False
      TabOrder = 2
    end
    object editHeartbeatMin: TMaskEdit
      Left = 128
      Top = 32
      Width = 121
      Height = 21
      Hint = 
        'Specify the minimum number of heartbeats a song must play'#13#10'befor' +
        'e it is considered for logging by gen_playtime.'#13#10'This setting ta' +
        'kes effect when the current song ends.'#13#10' '#13#10'It is on this heartbe' +
        'at that most of the song data is logged.'#13#10'There is also a small ' +
        'amount of data logged when the song ends.'#13#10' '#13#10'The default settin' +
        'g of 1 means the song is considered alive'#13#10'on the first heartbea' +
        't. This is a bit deceiving though, because'#13#10'counting starts at z' +
        'ero. Heartbeat 1 is actually the second'#13#10'heartbeat, but it is th' +
        'e first guaranteed full heartbeat. Because'#13#10'of this, it is also ' +
        'guaranteed that setting this to 0 will result in'#13#10'some incorrect' +
        ' data in logs.'
      EditMask = '09999;0; '
      MaxLength = 5
      ParentShowHint = False
      ShowHint = False
      TabOrder = 1
    end
    object editLogname: TEdit
